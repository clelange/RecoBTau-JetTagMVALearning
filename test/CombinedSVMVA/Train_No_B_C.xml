<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<MVATrainer>
	<general>
		<!--
			called with arguments
				1. name
				2. additional name info
				3. file suffix
			%n$s retrieves the n'th argument
		-->
		<option name="trainfiles">train_No_B_C_%1$s%2$s.%3$s</option>
		<option name="id">CombinedSVNoVertex</option>
	</general>
	<input id="input">
		<var name="jetPt" multiple="false" optional="false"/>
		<var name="jetEta" multiple="false" optional="false"/>
		<var name="vertexCategory" multiple="false" optional="false"/>
		<var name="trackSip2dSig" multiple="true" optional="true"/>
		<var name="trackSip3dSig" multiple="true" optional="true"/>
		<var name="trackSip2dVal" multiple="true" optional="true"/>
		<var name="trackSip3dVal" multiple="true" optional="true"/>
		<var name="trackMomentum" multiple="true" optional="true"/>
		<var name="trackEta" multiple="true" optional="true"/>
		<var name="trackPtRel" multiple="true" optional="true"/>
		<var name="trackPPar" multiple="true" optional="true"/>
		<var name="trackEtaRel" multiple="true" optional="true"/>
		<var name="trackDeltaR" multiple="true" optional="true"/>
		<var name="trackPtRatio" multiple="true" optional="true"/>
		<var name="trackPParRatio" multiple="true" optional="true"/>
		<var name="trackJetDist" multiple="true" optional="true"/>
		<var name="trackFirstTrackDist" multiple="true" optional="true"/>
		<var name="trackDecayLenVal" multiple="true" optional="true"/>
		<var name="trackSip2dSigAboveCharm" multiple="false" optional="false"/>
		<var name="trackSip3dSigAboveCharm" multiple="false" optional="false"/>
		<var name="trackSumJetEtRatio" multiple="false" optional="false"/>
		<var name="trackSumJetDeltaR" multiple="false" optional="false"/>
		<var name="nTracks" multiple="false" optional="false"/>
		<var name="category" multiple="false" optional="false"/>
	</input>
	<processor id="category" name="ProcCategory">
		<input>
			<var source="input" name="jetPt"/>
			<var source="input" name="jetEta"/>
		</input>
		<config>
			<group><box><range max="40"/><range min="-1.2" max="1.2"/></box></group>
			<group><box><range max="40"/><range min="-2.1" max="2.1"/></box></group>
			<group><box><range max="40"/><range/></box></group>
			<group><box><range max="60"/><range min="-1.2" max="1.2"/></box></group>
			<group><box><range max="60"/><range min="-2.1" max="2.1"/></box></group>
			<group><box><range max="60"/><range/></box></group>
			<group><box><range max="90"/><range min="-1.2" max="1.2"/></box></group>
			<group><box><range max="90"/><range min="-2.1" max="2.1"/></box></group>
			<group><box><range max="90"/><range/></box></group>
			<group><box><range max="150"/><range min="-1.2" max="1.2"/></box></group>
			<group><box><range max="150"/><range min="-2.1" max="2.1"/></box></group>
			<group><box><range max="150"/><range/></box></group>
			<group><box><range/><range min="-1.2" max="1.2"/></box></group>
			<group><box><range/><range min="-2.1" max="2.1"/></box></group>
			<group><box><range/><range/></box></group>
		</config>
		<output>
			<var name="category"/>
		</output>
	</processor>
	<processor id="count" name="ProcCount">
		<input>
			<var source="input" name="trackSip2dSig"/>
		</input>
		<config/>
		<output>
			<var name="nTracks"/>
		</output>
	</processor>
	<processor id="norm" name="ProcNormalize">
		<input>
			<var source="input" name="jetPt"/>
			<var source="input" name="jetEta"/>
			<var source="input" name="trackSip2dSig"/>
			<var source="input" name="trackSip3dSig"/>
			<var source="input" name="trackSip2dVal"/>
			<var source="input" name="trackSip3dVal"/>
			<var source="input" name="trackMomentum"/>
			<var source="input" name="trackDeltaR"/>
			<var source="input" name="trackDecayLenVal"/>
			<var source="input" name="trackSip2dSigAboveCharm"/>
			<var source="input" name="trackSip3dSigAboveCharm"/>
			<var source="count" name="nTracks"/>
		</input>
		<config>
			<pdf/>
			<pdf/>
			<pdf smooth="1" lower="-80" upper="150"/>
			<pdf smooth="1" lower="-80" upper="150"/>
			<pdf smooth="1" lower="-0.2" upper="0.2"/>
			<pdf smooth="1" lower="-3" upper="3"/>
			<pdf smooth="1" lower="0" upper="250"/>
			<pdf smooth="2" lower="0" upper="0.5"/>
			<pdf smooth="1" lower="0" upper="40"/>
			<pdf smooth="1" lower="-50" upper="80"/>
			<pdf smooth="1" lower="-50" upper="80"/>
			<pdf lower="0" upper="25"/>
		</config>
		<output>
			<var name="jetPt"/>
			<var name="jetEta"/>
			<var name="trackSip2dSig"/>
			<var name="trackSip3dSig"/>
			<var name="trackSip2dVal"/>
			<var name="trackSip3dVal"/>
			<var name="trackMomentum"/>
			<var name="trackDeltaR"/>
			<var name="trackDecayLenVal"/>
			<var name="trackSip2dSigAboveCharm"/>
			<var name="trackSip3dSigAboveCharm"/>
			<var name="nTracks"/>
		</output>
	</processor>
	<processor id="split" name="ProcSplitter">
		<input>
			<var source="norm" name="trackSip3dSig"/>
		</input>
		<config>
			<select first="3"/>
		</config>
		<output>
			<var name="trackSip3dSig1"/>
			<var name="trackSip3dSig2"/>
			<var name="trackSip3dSig3"/>
			<var name="trackSip3dSig"/>
		</output>
	</processor>

	<processor id="preLkh2" name="ProcLikelihood">
		<input>
			<var source="category" name="category"/>
			<var source="split" name="trackSip3dSig"/>
		</input>
		<config>
			<general strict="true"/>
			<category count="15"/>
			<sigbkg smooth="3"/>
		</config>
		<output>
			<var name="trackSip3dSig"/>
		</output>
	</processor>
	<processor id="opt" name="ProcOptional">
		<input>
			<var source="split" name="trackSip3dSig1"/>
			<var source="split" name="trackSip3dSig2"/>
			<var source="split" name="trackSip3dSig3"/>
			<var source="preLkh2" name="trackSip3dSig"/>
		</input>
		<config>
			<neutral pos="-0.1"/>
			<neutral pos="-0.1"/>
			<neutral pos="-0.1"/>
			<neutral pos="-0.1"/>
		</config>
		<output>
			<var name="trackSip3dSig1"/>
			<var name="trackSip3dSig2"/>
			<var name="trackSip3dSig3"/>
			<var name="trackSip3dSig"/>
		</output>
	</processor>
<!--
	<processor id="lkh" name="ProcLikelihood">
		<input>
			<var source="category" name="category"/>
			<var source="norm" name="trackEtaRel"/>
			<var source="norm" name="vertexMass"/>
			<var source="norm" name="vertexNTracks"/>
			<var source="norm" name="vertexEnergyRatio"/>
			<var source="norm" name="trackSip2dSigAboveCharm"/>
			<var source="norm" name="flightDistance2dSig"/>
			<var source="split" name="trackSip3dSig1"/>
			<var source="split" name="trackSip3dSig2"/>
			<var source="split" name="trackSip3dSig3"/>
			<var source="split" name="trackSip3dSig"/>
		</input>
		<config>
			<general log="true"/>
			<category count="15"/>
			<sigbkg smooth="3"/>
			<sigbkg smooth="3"/>
			<sigbkg/>
			<sigbkg smooth="3"/>
			<sigbkg smooth="3"/>
			<sigbkg smooth="3"/>
			<sigbkg smooth="3"/>
			<sigbkg smooth="3"/>
			<sigbkg smooth="3"/>
			<sigbkg smooth="3"/>
		</config>
		<output>
			<var name="discriminator"/>
		</output>
	</processor>
-->
	<processor id="mlp1" name="ProcMLP">
		<input>
			<var source="norm" name="jetPt"/>
			<var source="norm" name="jetEta"/>
			<var source="norm" name="trackSip2dSigAboveCharm"/>
			<var source="norm" name="nTracks"/>
			<var source="opt" name="trackSip3dSig1"/>
			<var source="opt" name="trackSip3dSig2"/>
			<var source="opt" name="trackSip3dSig3"/>
			<var source="opt" name="trackSip3dSig"/>
		</input>
		<config>
			<config steps="200" limiter="50">20:15</config>
		</config>
		<output>
			<var name="discriminator"/>
		</output>
	</processor>
<!--
	<processor id="mlp" name="ProcMLP">
		<input>
			<var source="norm" name="jetPt"/>
			<var source="norm" name="jetEta"/>
			<var source="norm" name="trackSip2dSigAboveCharm"/>
			<var source="norm" name="nTracks"/>
			<var source="opt" name="trackSip3dSig1"/>
			<var source="opt" name="trackSip3dSig2"/>
			<var source="opt" name="trackSip3dSig3"/>
			<var source="opt" name="trackSip3dSig"/>
			<var source="mlp1" name="discriminator"/>
		</input>
		<config>
			<config steps="200" limiter="60" booster="8">20:15</config>
		</config>
		<output>
			<var name="discriminator"/>
		</output>
	</processor>
-->
	<processor id="post" name="ProcNormalize">
		<input>
			<var source="mlp1" name="discriminator"/>
		</input>
		<config>
			<pdf lower="-0.5" upper="1.5" size="200" smooth="1"/>
		</config>
		<output>
			<var name="discriminator"/>
		</output>
	</processor>
	<processor id="comb" name="ProcLikelihood">
		<input>
			<var source="category" name="category"/>
			<var source="post" name="discriminator"/>
		</input>
		<config>
			<bias_table>
				<bias>0.047423</bias>
				<bias>0.052858</bias>
				<bias>0.065251</bias>
				<bias>0.029388</bias>
				<bias>0.035445</bias>
				<bias>0.043223</bias>
				<bias>0.027180</bias>
				<bias>0.034122</bias>
				<bias>0.038688</bias>
				<bias>0.027720</bias>
				<bias>0.035360</bias>
				<bias>0.040304</bias>
				<bias>0.034771</bias>
				<bias>0.043993</bias>
				<bias>0.046142</bias>
			</bias_table>
			<category count="15"/>
			<sigbkg lower="0" upper="1" size="200" smooth="0"/>
		</config>
		<output>
			<var name="discriminator"/>
		</output>
	</processor>
	<output>
		<var source="comb" name="discriminator"/>
	</output>
</MVATrainer>
